package com.SpringFramework.learn_springboot;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Component;

@Component
public class Dev {

//	@Autowired
	Laptop laptop;

	Dev(Laptop laptop) {
		this.laptop = laptop;
	}

	public void build() {
		laptop.compile();
		System.out.println("Coding in Spring Boot!");
	}
}
